@inject Microsoft.ApplicationInsights.Extensibility.TelemetryConfiguration TelemetryConfiguration
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - UserManagement</title>

    <link href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap.min.css" rel="stylesheet" asp-copy-src-to-fallback="development" />
    <link href="//datatables.net/release-datatables/extensions/Plugins/integration/bootstrap/3/dataTables.bootstrap.css" rel="stylesheet" asp-copy-src-to-fallback="development" />
    <link href="~/lib/bootstrap-material-design/dist/css/roboto.min.css" rel="stylesheet" asp-copy-src-to-fallback="development" />
    <link href="~/lib/bootstrap-material-design/dist/css/material.min.css" rel="stylesheet" asp-copy-src-to-fallback="development" />
    <link href="~/lib/bootstrap-material-design/dist/css/ripples.min.css" rel="stylesheet" asp-copy-src-to-fallback="development" />
    <link href="//fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

    <link rel="stylesheet" href="~/css/site.css" />
    <link href="~/css/bootstrapMD-datetimepicker.css" rel="stylesheet">
    @Html.ApplicationInsightsJavaScript(TelemetryConfiguration)
</head>
<body>
    <div class="navbar navbar-default navbar-fixed">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-responsive-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="javascript:void(0)">Usermanagement</a>
        </div>
        <div class="navbar-collapse collapse navbar-responsive-collapse">
            <ul class="nav navbar-nav">
                <li><a asp-controller="Home" asp-action="Index">Home</a></li>
                <li><a asp-controller="User" asp-action="Index">Users</a></li>
                <li><a asp-controller="Home" asp-action="Contact">Contact</a></li>
            </ul>
            @await Html.PartialAsync("_LoginPartial")
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; 2015 - UserManagement</p>
        </footer>
    </div>

    <script src="//datatables.net/release-datatables/media/js/jquery.js"></script>
    <script src="~/js/site.js" asp-file-version="false"></script>

    <script src="//ajax.aspnetcdn.com/ajax/bootstrap/3.0.0/bootstrap.js" asp-copy-src-to-fallback="development" asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal"></script>
    <script src="//datatables.net/release-datatables/media/js/jquery.dataTables.js" asp-copy-src-to-fallback="development" asp-fallback-test="window.jQuery().dataTable" asp-warn-if-test-is-invalid="development"></script>
    <script src="//datatables.net/release-datatables/extensions/TableTools/js/dataTables.tableTools.js" asp-copy-src-to-fallback="development" asp-fallback-test="window.jQuery().dataTable.TableTools" asp-warn-if-test-is-invalid="development"></script>
    <script src="//datatables.net/release-datatables/extensions/Plugins/integration/bootstrap/3/dataTables.bootstrap.js" asp-copy-src-to-fallback="development" asp-fallback-test="window.jQuery().dataTable.ext.renderer.pageButton.bootstrap" asp-warn-if-test-is-invalid="development"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/moment.js/2.9.0/moment-with-locales.js" asp-copy-src-to-fallback="development"></script>
    <script src="~/lib/bootstrap-material-design/dist/js/ripples.min.js" asp-copy-src-to-fallback="development"></script>
    <script src="~/lib/bootstrap-material-design/dist/js/material.min.js" asp-copy-src-to-fallback="development"></script>
    <script src="~/lib/jquery.are-you-sure/jquery.are-you-sure.js" asp-copy-src-to-fallback="development"></script>
    <script src="~/js/mvc.jquery.datatables.js" asp-copy-src-to-fallback="development"></script>
    <script src="~/js/bootstrapMD-datetimepicker.js" asp-copy-src-to-fallback="development"></script>

    <script>
        $(document).ready(function () {
            // This command is used to initialize some elements and make them work properly
            $.material.init();
        });
    </script>

    @RenderSection("scripts", required: false)
</body>
</html>
